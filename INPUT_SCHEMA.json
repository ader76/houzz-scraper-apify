{
  "title": "Houzz Scraper Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "location": {
      "title": "Location (city, state)",
      "type": "string",
      "editor": "textfield",
      "default": "Cambridge, MD"
    },
    "keyword": {
      "title": "Search keyword",
      "type": "string",
      "editor": "textfield",
      "default": "general contractor"
    },
    "maxPages": {
      "title": "Max pages",
      "type": "integer",
      "editor": "number",
      "default": 3
    }
  },
  "required": ["location", "keyword"]
}
